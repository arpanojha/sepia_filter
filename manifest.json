{
"manifest_version": 2,
"name": "Sepia_filter",
"version": "1.0.0",

"description": "Adds red to all webpages",

"content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["/content_script.js"]
  }
],
 "browser_action": {
   "browser_style": true,
   "default_icon": {
     "48": "border-48.png"
   },
   "default_title": "Sepia_filter",
   "default_popup": "popup.html"
 },
 "permissions":[
   "storage"
 ]

}
